<h1>Shopping Cart</h1>
<table>
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Price</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        <% for (let item of cart) { %>
            <tr>
                <td>
                    <%= item.name %>
                </td>
                <td>
                    <%= item.price %>
                </td>
                <td>
                    <%= item.quantity %>
                </td>
            </tr>
            <% } %>
    </tbody>
</table>